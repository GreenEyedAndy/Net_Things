<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--general-->
    <SolidColorBrush x:Key="DefaultBackground" Color="#FFF6F6F6"/>
    <SolidColorBrush x:Key="DefaultForeground" Color="Black"/>

    <!--modules-->
    <SolidColorBrush x:Key="DefaultModuleBackground" Color="#FFF6F6F6"/>
    <SolidColorBrush x:Key="DefaultModuleForeground" Color="Black"/>

    <!--header-->
    <SolidColorBrush x:Key="HeaderBackground" Color="#FF204E7E"/>
    <SolidColorBrush x:Key="HeaderForeground" Color="White"/>
    <SolidColorBrush x:Key="HeaderBorderBrush" Color="#FFA6A6A6"/>

    <!--button-->
    <SolidColorBrush x:Key="ButtonBackground" Color="#FF204E7E"/>
    <SolidColorBrush x:Key="ButtonForeground" Color="White"/>

    <!--statusbar-->
    <SolidColorBrush x:Key="StatusBarBackground" Color="#FF2E7FC2"/>
    <SolidColorBrush x:Key="StatusBarForeground" Color="White"/>

</ResourceDictionary>
